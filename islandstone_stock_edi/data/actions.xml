        <record id="schedule_action_update_build_report_islandstone" model="ir.cron">
            <field name="name">Product Report: Send Build.com Product Inventory Report to Email</field>
            <field name="model_id" ref="islandstone_stock_edi.model_product_report"/>
            <field name="state">code</field>
            <field name="code">model.send_build_report()
