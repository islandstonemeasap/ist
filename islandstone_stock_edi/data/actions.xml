<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="schedule_action_update_attributes_luonto" model="ir.cron">
            <field name="name">Product Report Wayfair: Send Email with XLSX</field>
            <field name="model_id" ref="product.model_product_report"/>
            <field name="state">email</field>
            <field name="template_id">model.create_attr_val_exclusions()
            </field>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="numbercall">-1</field>
            <field name="nextcall" eval="(DateTime.now() + timedelta(days=1)).strftime('%Y-%m-%d 2:00:00')" />
            <field name="doall" eval="False"/>
        </record>


    </data>
</odoo>